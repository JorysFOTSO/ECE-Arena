int ordreDuJeu(int compteur, int nbrjoueurs)
{
    if (nbrjoueurs == 2)
    {
        compteur = rand()%1;
    }
    else if (nbrjoueurs == 3)
    {
        compteur = rand()%2;
    }
    else if (nbrjoueurs == 4)
    {
        compteur = rand()%3;
    }
    return compteur;
} //ne pas oublier de mettre la biblio time.h et initialiser srand(time(NULL)
